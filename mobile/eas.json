{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "API_URL": "http://localhost:8000",
        "API_PREFIX": "/api"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "API_URL": "https://api.yourdomain.com",
        "API_PREFIX": ""
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "API_URL": "https://api.yourdomain.com",
        "API_PREFIX": ""
      }
    }
  },
  "submit": {
    "production": {}
  }
}
